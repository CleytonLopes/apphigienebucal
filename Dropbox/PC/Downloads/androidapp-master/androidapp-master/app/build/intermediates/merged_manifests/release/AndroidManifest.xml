<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.apptela"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="31" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:icon="@drawable/dente"
        android:label="@string/app_name"
        android:roundIcon="@drawable/dente"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppTela" >
        <activity
            android:name="com.example.apptela.NonaActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.apptela.OitavaActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.apptela.SetimaActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.apptela.SextaActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.apptela.QuintaActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.apptela.QuartaActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.apptela.TerceiraActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.apptela.SegundaActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.apptela.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>